class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        common_strs = ''

        for i in range(len(strs[0])):
            for word in strs:
                if i == len(word) or word[i] != strs[0][i]:
                    return common_strs
                print(word,word[i],strs[0][i])
            common_strs += strs[0][i]
        return common_strs
            
